.add-to-cart-button {
    text-align: center;
}
/*đổi màu xem nhanh*/
.grid-tools a {
    background-color: #54aa00;
    color: #f1f1f1;
    display: block;
    font-size: .85em;
    font-weight: 700;
    opacity: .95;
    padding: .4em 0 .5em;
    text-overflow: ellipsis;
    text-transform: uppercase;
    transition: opacity .3s;
    width: 100%;
    border-radius: 25px 25px 25px 25px;
}

/*đổi màu thêm vào giỏ hàng*/
.primary, input[type=submit], input[type=submit].button {
    background-color: #54aa00;
	border-radius: 14px
}
/*đổi màu link tên sản phẩm*/
a.woocommerce-LoopProduct-link.woocommerce-loop-product__link {
    color: #e0142f;
		font-weight: 600
}
/*đổi màu khung tròn*/
.is-outline {
    color: #e0142f;
}
.is-outline:hover, .primary.is-outline:hover {
    background-color: #b20000;
    border-color: #fff;
    color: #fff;
}

/*đổi màu tiêu đề sản phẩm*/
h1.product-title.product_title.entry-title {
    color: #54aa00;
}
/*đổi màu thêm giỏ hàng*/
button.single_add_to_cart_button.button.alt {
    background-color: #b20000;
}
/*đổi màu đường line*/
.nav-line-bottom>li>a:before, .nav-line-grow>li>a:before, .nav-line>li>a:before {
    background-color: #54aa00;
    content: " ";
    height: 3px;
    left: 50%;
    opacity: 0;
    position: absolute;
    top: 0;
    transform: translateX(-50%);
    transition: all .3s;
    width: 100%;
}
/*đổi màu text sau khi thêm giỏ hàng*/
a.added_to_cart.wc-forward {
    color: #e0142f;
}
/*đổi màu khi đến giỏ hàng*/
a.elementor-button.elementor-button--view-cart.elementor-size-md {
    background-color: #54aa00;
}
a.elementor-button.elementor-button--checkout.elementor-size-md {
    background-color: #54aa00;
}
/*đổi màu text tạm tính*/
strong {
    color: #54aa00;
}
/*đổi màu tên sản phẩm trong giỏ hàng*/
.elementor-widget-woocommerce-menu-cart .elementor-menu-cart__product-name a {
    font-family: var(--e-global-typography-primary-font-family), Nunito;
    font-weight: var(--e-global-typography-primary-font-weight);
    text-transform: var(--e-global-typography-primary-text-transform);
    color: #e0142f;
}
/*đổi màu cập nhật giỏ hàng*/
.button.disabled, .button[disabled], button.disabled, button[disabled] {
    opacity: 1;
}
a.button-continue-shopping.button.primary.is-outline {
    border-color: #54aa00;
}
a.button-continue-shopping.button.primary.is-outline {
    color: #54aa00;
}
.button.alt, .button.checkout, .checkout-button, .secondary {
    background-color: #e0142f;
}
.is-outline:hover, .primary.is-outline:hover {
    background-color: #b2000000;
    border-color: #fff;
    color: #fff;
}

/*đổi màu số trang*/
.nav-pagination>li>.current, .nav-pagination>li>a:hover, .nav-pagination>li>span:hover {
    background-color: #54aa00;
    border-color: #54aa00;
    color: #fff;
}
/*đổi màu khung bình luận*/
.has-border {
    border: 2px solid #54AA00;
    padding: 15px 30px 30px;
}
/*đổi màu dòng chữ nhỏ trên cùng bài viết*/
a {
    color: #b11025;
    text-decoration: none;
}

/* đổi màu chữ văn bản*/
body {
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    color: #1e1c1c;
}
